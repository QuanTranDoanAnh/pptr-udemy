{
  "name": "pptr-udemy",
  "version": "1.0.0",
  "description": "Automated software testing with Puppeteer",
  "main": "index.js",
  "scripts": {
    "test": "mocha --timeout=30000 ./tests --reporter=list",
    "test:device": "mocha --timeout=30000 --reporter=list ./tests/device.test.js",
    "test:incognito": "mocha --timeout=30000 --reporter=list ./tests/incognito.test.js",
    "test:e2e": "mocha --timeout=30000 --reporter=list ./tests/e2e/*.test.js"
  },
  "keywords": [
    "Puppeteer",
    "Mocha",
    "Chai",
    "Automation"
  ],
  "author": "QuanTranDoanAnh",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.3.8",
    "mocha": "^10.2.0",
    "puppeteer": "^21.2.1"
  },
  "devDependencies": {
    "prettier": "^3.0.3"
  }
}
